    "svg",
    "icons",
    "sprite"
  ],
  "files": [
    "dist"
  ],
  "author": "Alexey Schebelev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexxNB/rollup-plugin-svg-icons/issues"
  },
  "homepage": "https://github.com/AlexxNB/rollup-plugin-svg-icons#readme",
  "devDependencies": {
    "bili": "^4.8.1",
    "open-cli": "^5.0.0",
    "rollup": "^1.23.1",
    "sirv-cli": "^0.4.4"
  },
  "dependencies": {
    "svgo": "^1.3.0",
    "svgstore": "^3.0.0-2"
  }
